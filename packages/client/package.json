{
  "name": "supakeys",
  "version": "0.1.7",
  "description": "Passkey authentication for Supabase - WebAuthn made simple",
  "author": "Alex Whitmore",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexwhitmore/supakeys"
  },
  "homepage": "https://supakeys.com",
  "bugs": {
    "url": "https://github.com/alexwhitmore/supakeys/issues"
  },
  "keywords": [
    "supabase",
    "passkeys",
    "webauthn",
    "fido2",
    "authentication",
    "passwordless",
    "biometric",
    "touchid",
    "faceid"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "supakeys": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^11.0.0",
    "@simplewebauthn/types": "^12.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "enquirer": "^2.4.1",
    "ora": "^7.0.1"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
